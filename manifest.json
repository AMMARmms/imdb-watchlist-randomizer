{
    "name": "Chrome Exte MV3",
    "description": "building chrome ext",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "./background.js"
    },
    "action": {
        "default_popup": "./popup.html"
    },
    "permissions": [
        "scripting",
        "storage",
        "tabs"
    ],
    "host_permissions": [
        "https://www.imdb.com/user/*/watchlist*"
    ]
}
